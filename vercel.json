{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "cleanUrls": true,
    "framework": null,
    "functions": {
      "api/*.js": {
        "runtime": "nodejs18.x",
        "memory": 128,
        "maxDuration": 10
      }
    },
    "redirects": [],
    "rewrites": [
      { "source": "/", "destination": "/public/index.html" }
    ]
  }
  